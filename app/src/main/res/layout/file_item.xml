<LinearLayout android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:orientation="horizontal"
	xmlns:android="http://schemas.android.com/apk/res/android">


	<ImageView android:id="@+id/file_item_icon"
		android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/file_icon"
		android:paddingRight="5dp"/>
	<TextView android:id="@+id/file_item_text"
		android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="text"></TextView>
</LinearLayout>
